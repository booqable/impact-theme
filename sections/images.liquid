{%- assign blocks = section.blocks -%}

{%- if blocks.size > 0 -%}
  {{ "images.css" | asset_url | stylesheet_tag }}

  {%- assign color_palette         = section.settings.color_palette -%}
  {%- assign height                = section.settings.height -%}
  {%- assign padding_top           = section.settings.padding_top -%}
  {%- assign padding_bottom        = section.settings.padding_bottom -%}
  {%- assign padding_top_mobile    = section.settings.padding_top_mobile -%}
  {%- assign padding_bottom_mobile = section.settings.padding_bottom_mobile -%}
  {%- assign timer                 = section.settings.timer -%}
  {%- assign pause                 = section.settings.pause -%}
  {%- assign effect                = section.settings.effect -%}

  {%- assign carousel = false -%}

  {%- if blocks.size > 1 -%}
    {{ "carousel.css" | asset_url | stylesheet_tag }}

    {%- assign carousel = true -%}

  {%- else -%}
    {%- assign overlay_color = nil -%}

    {%- for block in blocks -%}
      {%- assign overlay_color = block.settings.overlay_color -%}
    {%- endfor -%}
  {%- endif -%}

  {% comment %} CSS variables start {% endcomment %}
  {%- capture variables -%}
    {%- if carousel -%}
      --slide-width: 100%;
      --slide-width-mobile: 100%;
    {%- else -%}
      --overlay-color: {{- overlay_color -}};
    {%- endif -%}

    {%- case height -%}
      {%- when 'small' -%}
        --height: 730px;
      {%- when 'medium' -%}
        --height: 839px;
      {%- when 'full' -%}
        --height: 100vh;
    {%- endcase -%}

    {%- case padding_top -%}
      {%- when 'small' -%}
        --padding-top: 30px;
      {%- when 'medium' -%}
        --padding-top: 60px;
      {%- when 'large' -%}
        --padding-top: 100px;
    {%- endcase -%}

    {%- case padding_bottom -%}
      {%- when 'small' -%}
        --padding-bottom: 30px;
      {%- when 'medium' -%}
        --padding-bottom: 60px;
      {%- when 'large' -%}
        --padding-bottom: 100px;
    {%- endcase -%}

    {%- case padding_top_mobile -%}
      {%- when 'small' -%}
        --padding-top-mobile: 30px;
      {%- when 'medium' -%}
        --padding-top-mobile: 50px;
      {%- when 'large' -%}
        --padding-top-mobile: 80px;
    {%- endcase -%}

    {%- case padding_bottom_mobile -%}
      {%- when 'small' -%}
        --padding-bottom-mobile: 30px;
      {%- when 'medium' -%}
        --padding-bottom-mobile: 50px;
      {%- when 'large' -%}
        --padding-bottom-mobile: 80px;
    {%- endcase -%}
  {%- endcapture -%}
  {% comment %} CSS variables end {% endcomment %}

  {%- assign default_color = nil -%}

  {%- if color_palette == "one" -%}
    {%- assign default_color = settings.primary_color_1 | default : #1A3333 -%}
  {%- elsif color_palette == "two" -%}
    {%- assign default_color = settings.primary_color_2 | default : #1A3333 -%}
  {%- elsif color_palette == "three" -%}
    {%- assign default_color = settings.primary_color_3 | default : #F6F4F2 -%}
  {%- elsif color_palette == "four" -%}
    {%- assign default_color = settings.primary_color_4 | default : #F6F4F2 -%}
  {%- elsif color_palette == "five" -%}
    {%- assign default_color = settings.primary_color_5 | default : #1A3333 -%}
  {%- endif -%}

  <div class="images__wrapper{% if height == "full" or height == "medium" %} images__wrapper-full-height{% endif %}{% if color_palette != blank %} palette-{{ color_palette }}{%- endif -%}" style="{{- variables | escape -}}">
    {%- if carousel -%}
      <div class="carousel carousel__edges carousel__{{- effect -}}-effect{% if pause %} carousel__pause{%- endif -%}" aria-role="Gallery" data-dafault-color="{{- default_color -}}">
        <input class="carousel__timer" type="hidden" name="hidden" value="{{- timer -}}" aria-hidden="true">
        <div class="carousel__wrapper">
    {%- endif -%}

          {%- for block in blocks -%}
            {%- assign first                = forloop.first -%}
            {%- assign tag                  = block.settings.tag -%}
            {%- assign title                = block.settings.title -%}
            {%- assign description          = block.settings.description -%}
            {%- assign button_1             = block.settings.button_1 -%}
            {%- assign button_url_1         = block.settings.button_url_1 -%}
            {%- assign button_2             = block.settings.button_2 -%}
            {%- assign button_url_2         = block.settings.button_url_2 -%}
            {%- assign text_position        = block.settings.text_position -%}
            {%- assign text_position_mobile = block.settings.text_position_mobile -%}
            {%- assign video_url            = block.settings.video_url | strip -%}
            {%- assign image                = block.settings.image -%}
            {%- assign image_mobile         = block.settings.image_mobile -%}
            {%- assign overlay_opacity      = block.settings.overlay_opacity -%}
            {%- assign overlay_background   = block.settings.overlay_background -%}
            {%- assign overlay_color        = block.settings.overlay_color -%}
            {%- assign show_overlay         = block.settings.show_overlay -%}

            {% comment %} CSS variables start {% endcomment %}
            {%- capture text_variables -%}
              {% case text_position %}
                {%- when 'top-left' -%}
                  --text-justify-content: flex-start;
                  --text-align-items: flex-start;
                  --text-align: left;
                  --margin-bottom: 70px;
                  --margin-left: 0;
                  --margin-right: auto;
                {%- when 'top-center' -%}
                  --text-justify-content: center;
                  --text-align-items: flex-start;
                  --text-align: center;
                  --margin-bottom: 70px;
                  --margin-left: auto;
                  --margin-right: auto;
                {%- when 'top-right' -%}
                  --text-justify-content: flex-end;
                  --text-align-items: flex-start;
                  --text-align: right;
                  --margin-bottom: 70px;
                  --margin-left: auto;
                  --margin-right: 0;
                {%- when 'middle-left' -%}
                  --text-justify-content: flex-start;
                  --text-align-items: center;
                  --text-align: left;
                  --margin-bottom: 70px;
                  --margin-left: 0;
                  --margin-right: auto;
                {%- when 'center' -%}
                  --text-justify-content: center;
                  --text-align-items: center;
                  --text-align: center;
                  --margin-bottom: 70px;
                  --margin-left: auto;
                  --margin-right: auto;
                {%- when 'middle-right' -%}
                  --text-justify-content: flex-end;
                  --text-align-items: center;
                  --text-align: right;
                  --margin-bottom: 70px;
                  --margin-left: auto;
                  --margin-right: 0;
                {%- when 'bottom-left' -%}
                  --text-justify-content: flex-start;
                  --text-align-items: flex-end;
                  --text-align: left;
                  --margin-bottom: 70px;
                  --margin-left: 0;
                  --margin-right: auto;
                {%- when 'bottom-center' -%}
                  --text-justify-content: center;
                  --text-align-items: flex-end;
                  --text-align: center;
                  --margin-bottom: 70px;
                  --margin-left: auto;
                  --margin-right: auto;
                {%- when 'bottom-right' -%}
                  --text-justify-content: flex-end;
                  --text-align-items: flex-end;
                  --text-align: right;
                  --margin-bottom: 70px;
                  --margin-left: auto;
                  --margin-right: 0;
              {% endcase %}

              {% case text_position_mobile %}
                {%- when 'top-left' -%}
                  --text-justify-content-mobile: flex-start;
                  --text-align-items-mobile: flex-start;
                  --text-align-mobile: left;
                  --margin-bottom-mobile: 60px;
                  --margin-left-mobile: 0;
                  --margin-right-mobile: auto;
                {%- when 'top-center' -%}
                  --text-justify-content-mobile: center;
                  --text-align-items-mobile: flex-start;
                  --text-align-mobile: center;
                  --margin-bottom-mobile: 60px;
                  --margin-left-mobile: auto;
                  --margin-right-mobile: auto;
                {%- when 'top-right' -%}
                  --text-justify-content-mobile: flex-end;
                  --text-align-items-mobile: flex-start;
                  --text-align-mobile: right;
                  --margin-bottom-mobile: 60px;
                  --margin-left-mobile: auto;
                  --margin-right-mobile: 0;
                {%- when 'middle-left' -%}
                  --text-justify-content-mobile: flex-start;
                  --text-align-items-mobile: center;
                  --text-align-mobile: left;
                  --margin-bottom-mobile: 60px;
                  --margin-left-mobile: 0;
                  --margin-right-mobile: auto;
                {%- when 'center' -%}
                  --text-justify-content-mobile: center;
                  --text-align-items-mobile: center;
                  --text-align-mobile: center;
                  --margin-bottom-mobile: 60px;
                  --margin-left-mobile: auto;
                  --margin-right-mobile: auto;
                {%- when 'middle-right' -%}
                  --text-justify-content-mobile: flex-end;
                  --text-align-items-mobile: center;
                  --text-align-mobile: right;
                  --margin-bottom-mobile: 60px;
                  --margin-left-mobile: auto;
                  --margin-right-mobile: 0;
                {%- when 'bottom-left' -%}
                  --text-justify-content-mobile: flex-start;
                  --text-align-items-mobile: flex-end;
                  --text-align-mobile: left;
                  --margin-bottom-mobile: 60px;
                  --margin-left-mobile: 0;
                  --margin-right-mobile: auto;
                {%- when 'bottom-center' -%}
                  --text-justify-content-mobile: center;
                  --text-align-items-mobile: flex-end;
                  --text-align-mobile: center;
                  --margin-bottom-mobile: 60px;
                  --margin-left-mobile: auto;
                  --margin-right-mobile: auto;
                {%- when 'bottom-right' -%}
                  --text-justify-content-mobile: flex-end;
                  --text-align-items-mobile: flex-end;
                  --text-align-mobile: right;
                  --margin-bottom-mobile: 60px;
                  --margin-left-mobile: auto;
                  --margin-right-mobile: 0;
              {% endcase %}

              {% if show_overlay %}
                --overlay-color: {{- overlay_color -}};
              {%- endif -%}
            {%- endcapture -%}

            {%- if show_overlay -%}
              {%- capture image_variables -%}
                --overlay-opacity: {{ overlay_opacity | append: "%"}};
                --overlay-background: {{- overlay_background -}};
                --overlay-background-00: {{- overlay_background | append: "00" -}};

                {% case text_position %}
                  {%- when 'top-left' -%}
                    --direction-overlay: 127deg;
                  {%- when 'top-center' -%}
                    --direction-overlay: 180deg;
                  {%- when 'top-right' -%}
                    --direction-overlay: 217deg;
                  {%- when 'middle-left' -%}
                    --direction-overlay: 127deg;
                  {%- when 'center' -%}
                    --direction-overlay: 180deg;
                  {%- when 'middle-right' -%}
                    --direction-overlay: 217deg;
                  {%- when 'bottom-left' -%}
                    --direction-overlay: 37deg;
                  {%- when 'bottom-center' -%}
                    --direction-overlay: 0deg;
                  {%- when 'bottom-right' -%}
                    --direction-overlay: 307deg;
                {% endcase %}

                {% case text_position_mobile %}
                  {%- when 'top-left' -%}
                    --direction-overlay-mobile: 127deg;
                  {%- when 'top-center' -%}
                    --direction-overlay-mobile: 180deg;
                  {%- when 'top-right' -%}
                    --direction-overlay-mobile: 217deg;
                  {%- when 'middle-left' -%}
                    --direction-overlay-mobile: 127deg;
                  {%- when 'center' -%}
                    --direction-overlay-mobile: 180deg;
                  {%- when 'middle-right' -%}
                    --direction-overlay-mobile: 217deg;
                  {%- when 'bottom-left' -%}
                    --direction-overlay-mobile: 37deg;
                  {%- when 'bottom-center' -%}
                    --direction-overlay-mobile: 0deg;
                  {%- when 'bottom-right' -%}
                    --direction-overlay-mobile: 307deg;
                {% endcase %}
              {%- endcapture -%}
            {%- endif -%}
            {% comment %} CSS variables end {% endcomment %}

            {%- if carousel -%}
              <div class="carousel__item{% if effect == 'fade' and first == false %} hide{% endif %}{% if effect == 'fade' and first %} show{%- endif -%}"{% if show_overlay %} data-overlay-color="{{- overlay_color | escape -}}"{%- endif -%}>
                <div class="carousel__inner">
            {%- endif -%}
                  <div
                    class="images__container container{% if padding_top != blank or padding_top_mobile != blank %} images__container--padding-top{% endif %}{% if padding_bottom != blank or padding_bottom_mobile != blank %} images__container--padding-bottom{%- endif -%}"
                    style="{{- text_variables | escape -}}"
                  >
                    <div class="images__text-area{% if show_overlay %} images__text-area--color{%- endif -%}">
                      <div class="images__content">
                        {%- if tag != blank -%}
                          <strong class="images__tag tagline">{{- tag -}}</strong>
                        {%- endif -%}

                        {%- if title != blank -%}
                          {%- if carousel -%}
                            <h2 class="images__title bq-content rx-content">{{- title -}}</h2>
                          {%- else -%}
                            <h1 class="images__title bq-content rx-content">{{- title -}}</h1>
                          {%- endif -%}
                        {%- endif -%}

                        {%- if description != blank -%}
                          <div class="images__description text-medium bq-content rx-content">{{- description | truncatewords: 20, " ..." -}}</div>
                        {%- endif -%}

                        {%- if button_1 != blank or button_2 != blank -%}
                          <div class="images__buttons">
                            {%- if button_1 != blank and button_url_1 != blank -%}
                              <a href="{{ button_url_1 }}" class="images__button button button--primary button--large">{{- button_1 -}}</a>
                            {%- endif -%}

                            {%- if button_2 != blank and button_url_2 != blank -%}
                              <a href="{{ button_url_2 }}" class="images__button button button--outlined button--large">{{- button_2 -}}</a>
                            {%- endif -%}
                          </div>
                        {%- endif -%}

                      </div>
                    </div>
                  </div>

                  <div class="images__vision"{% if show_overlay %} style="{{- image_variables | escape -}}"{%- endif -%}">
                    {%- if video_url contains "youtube.com" -%}
                      {%- assign video_drop = video_url | split: "?v=" -%}
                      {%- assign video_id   = video_drop | last | strip -%}

                      <div class="images__vision-wrapper{% if show_overlay %} images__vision-wrapper--overlay{%- endif -%}">
                        <iframe
                          class="images__video"
                          width="1920"
                          height="1080"
                          src="https://www.youtube.com/embed/{{- video_id -}}?playlist={{- video_id -}}&autoplay=1&mute=1&loop=1&controls=0&disablekb=1&fs=0&playsinline=1&enablejsapi=1&iv_load_policy=3&modestbranding=1"
                          frameborder="0"
                          allow="autoplay; muted; encrypted-media; fullscreen"
                          allowfullscreen
                          autoplay
                          loop
                          mute
                          playsinline
                        ></iframe>
                      </div>

                    {%- elsif video_url contains "vimeo.com" -%}
                      {%- assign video_drop = video_url | split: "/" -%}
                      {%- assign video_id   = video_drop | last | strip -%}

                      <div class="images__vision-wrapper{% if show_overlay %} images__vision-wrapper--overlay{%- endif -%}">
                        <iframe
                          class="images__video"
                          src="https://player.vimeo.com/video/{{- video_id -}}?autoplay=1&muted=1&loop=1&autopause=0&controls=0&background=1"
                          width="1920"
                          height="1080"
                          frameborder="0"
                          allow="autoplay; fullscreen"
                          allowfullscreen
                          autoplay
                          muted
                          loop
                        ></iframe>
                      </div>

                    {%- elsif image != blank or image_mobile != blank -%}
                      {%- if image != blank -%}
                        <div class="images__vision-wrapper{% if image_mobile != blank %} images__vision-wrapper--desktop{% endif %}{% if show_overlay %} images__vision-wrapper--overlay{%- endif -%}">
                          {%- render 'image',
                            image: image,
                            loading: 'lazy',
                            class: 'images__image',
                            size: 'xxl'
                          -%}
                        </div>
                      {%- endif -%}

                      {%- if image_mobile != blank -%}
                        <div class="images__vision-wrapper images__vision-wrapper--mobile{% if show_overlay %} images__vision-wrapper--overlay{%- endif -%}">
                          {%- render 'image',
                            image: image,
                            loading: 'lazy',
                            class: 'images__image',
                            size: 'l'
                          -%}
                        </div>
                      {%- endif -%}
                    {%- endif -%}
                  </div>
            {%- if carousel -%}
                </div>
              </div>
            {%- endif -%}
          {%- endfor -%}

    {%- if carousel -%}
        </div>

        <div class="carousel__navigation" aria-role="Carousel navigation">
          <span class="carousel__btn prev" aria-label="Prev slide">
            Previous slide
            {%- render 'icon-arrow-right' -%}
          </span>
          <span class="carousel__btn next" aria-label="Next slide">
            Next slide
            {%- render 'icon-arrow-right' -%}
          </span>
        </div>

        <div class="carousel__pagination" aria-role="Carousel pagination">
          {%- for block in blocks -%}
            {%- assign first = forloop.first -%}
            {%- assign index = forloop.index -%}

            <span class="carousel__dot{% if first %} active{%- endif -%}" data-index="{{- index -}}" aria-label="Jump to slide {{ index }}">
              Go to slide {{ index }}
            </span>
          {%- endfor -%}
        </div>

        <div class="carousel__counter">
          {%- for block in blocks -%}
            {%- assign length = forloop.length -%}
          {%- endfor -%}

          <span class="carousel__count">{%- if length < 10 -%}0{%- endif -%}1</span>
          <span class="carousel__divider">|</span>
          <span class="carousel__summary">{%- if length < 10 -%}0{%- endif -%}{{- length -}}</span>
        </div>
      </div>
    {%- endif -%}
  </div>
{%- endif -%}

{% schema %}
  {
    "name": "Images",
    "tag": "section",
    "class": "images",
    "description": "Engage visitors by giving great first impression",
    "templates": ["index"],
    "blocks":[
      {
        "type": "slide",
        "name": "Slide",
        "settings": [
          {
            "type": "header",
            "content": "General settings"
          },
          {
            "type": "image_picker",
            "id": "image",
            "label": "Image",
            "default": "booqable://assets/image-hero.jpeg"
          },
          {
            "type": "image_picker",
            "id": "image_mobile",
            "label": "Image mobile (optional)"
          },
          {
            "type": "text",
            "id": "video_url",
            "label": "Video URL (optional)",
            "info": "Youtube, Vimeo. If you paste video link here, any images will be ignored"
          },
          {
            "type": "text",
            "id": "tag",
            "label": "Tagline",
            "info": "Leave blank to hide tagline"
          },
          {
            "type": "contentEditor",
            "id": "title",
            "label": "Title",
            "default": "Explore like <br> a daredevil"
          },
          {
            "type": "contentEditor",
            "id": "description",
            "label": "Description",
            "default": "Lorem ipsum dolor sit amet, consectetur adipiscing elit. Suspendisse <br> varius enim in eros elementum tristique."
          },
          {
            "type": "header",
            "content": "Image overlay"
          },
          {
            "type": "checkbox",
            "id": "show_overlay",
            "label": "Show overlay",
            "default": true
          },
          {
            "type": "color",
            "id": "overlay_background",
            "label": "Overlay background color",
            "default": "#1A3333"
          },
          {
            "type": "color",
            "id": "overlay_color",
            "label": "Overlay text color",
            "default": "#FFFFFF"
          },
          {
            "type": "number",
            "id": "overlay_opacity",
            "label": "Overlay percentage",
            "min": 0,
            "max": 100,
            "default": 72
          },
          {
            "type": "header",
            "content": "Buttons"
          },
          {
            "type": "text",
            "id": "button_1",
            "label": "Button 1 label",
            "default": "Check out products",
            "info": "Leave blank to hide the button"
          },
          {
            "type": "url",
            "id": "button_url_1",
            "label": "Button 1 link"
          },
          {
            "type": "text",
            "id": "button_2",
            "label": "Button 2 label",
            "default": "More info",
            "info": "Leave blank to hide the button"
          },
          {
            "type": "url",
            "id": "button_url_2",
            "label": "Button 2 link"
          },
          {
            "type": "header",
            "content": "Desktop settings"
          },
          {
            "type": "select",
            "id": "text_position",
            "label": "Text position",
            "options": [
              {
                "value": "top-left",
                "label": "Top / Left"
              },
              {
                "value": "top-center",
                "label": "Top / Center"
              },
              {
                "value": "top-right",
                "label": "Top / Right"
              },
              {
                "value": "middle-left",
                "label": "Middle / Left"
              },
              {
                "value": "center",
                "label": "Center"
              },
              {
                "value": "middle-right",
                "label": "Middle / Right"
              },
              {
                "value": "bottom-left",
                "label": "Bottom / Left"
              },
              {
                "value": "bottom-center",
                "label": "Bottom / Center"
              },
              {
                "value": "bottom-right",
                "label": "Bottom / Right"
              }
            ],
            "default": "middle-left"
          },
          {
            "type": "header",
            "content": "Mobile settings"
          },
          {
            "type": "select",
            "id": "text_position_mobile",
            "label": "Text position",
            "options": [
              {
                "value": "top-left",
                "label": "Top / Left"
              },
              {
                "value": "top-center",
                "label": "Top / Center"
              },
              {
                "value": "top-right",
                "label": "Top / Right"
              },
              {
                "value": "middle-left",
                "label": "Middle / Left"
              },
              {
                "value": "center",
                "label": "Center"
              },
              {
                "value": "middle-right",
                "label": "Middle / Right"
              },
              {
                "value": "bottom-left",
                "label": "Bottom / Left"
              },
              {
                "value": "bottom-center",
                "label": "Bottom / Center"
              },
              {
                "value": "bottom-right",
                "label": "Bottom / Right"
              }
            ],
            "default": "top-left"
          }
        ]
      }
    ],
    "max_blocks": 8,
    "settings": [
      {
        "type": "header",
        "content": "General settings"
      },
      {
        "type": "select",
        "id": "color_palette",
        "label": "Color palette",
        "options": [
          {
            "value": "one",
            "label": "Color set 1"
          },
          {
            "value": "two",
            "label": "Color set 2"
          },
          {
            "value": "three",
            "label": "Color set 3"
          },
          {
            "value": "four",
            "label": "Color set 4"
          },
          {
            "value": "five",
            "label": "Color set 5"
          }
        ],
        "default": "four"
      },
      {
        "type": "header",
        "content": "Carousel settings"
      },
      {
        "type": "number",
        "id": "timer",
        "label": "Rotation time",
        "min": 0,
        "max": 100,
        "default": 5,
        "info": "In seconds. Set to 0 to disable auto rotation"
      },
      {
        "type": "checkbox",
        "id": "pause",
        "default": true,
        "label": "Pause rotation on hover"
      },
      {
        "type": "select",
        "id": "effect",
        "label": "Slide effect",
        "options": [
          {
            "value": "fade",
            "label": "Fade"
          },
